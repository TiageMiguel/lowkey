{
  "name": "lowkey",
  "version": "1.0.0",
  "description": "Lowkey - Realtime Chat Web Application",
  "main": "main.js",
  "author": "Tiago Miguel",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "dev": "npm run server",
    "app": "electromon ./src/main.js",
    "server": "nodemon ./src/server.js --ignore src/router/static",
    "compile-package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=dist",
    "compile-package-win@ia32": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=dist",
    "compile-package-win@x64": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=dist",
    "compile-package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=dist"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "browser-detect": "^0.2.28",
    "connect": "^3.6.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "cryptr": "^4.0.2",
    "dotenv": "^6.1.0",
    "electron": "^4.1.4",
    "electron-packager": "^13.1.1",
    "email-validator": "^2.0.4",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.2",
    "express-rate-limit": "^3.4.1",
    "express-session": "^1.16.1",
    "express-socket.io-session": "^1.3.5",
    "helmet": "^3.16.0",
    "memorystore": "^1.6.1",
    "metagetter": "^1.0.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "nodemailer": "^4.6.8",
    "npm": "^6.9.0",
    "socket.io": "^2.1.1",
    "volleyball": "^1.5.1"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  }
}